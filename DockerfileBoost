FROM grandfleet/seng475-sde:HLBF2019

ENV INSTALL_DIR "/usr/local/sde"

WORKDIR /boost-install
COPY / .
RUN python3 --version && python --version

RUN echo $PATH

# Make new directory for installation
RUN chmod +x installerRest && ./installerRest -d INSTALL_DIR -e default -f

RUN cmake --version 
RUN gcc --version

RUN clang --version